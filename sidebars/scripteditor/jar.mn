
scripteditor.jar:
% content   scripteditor          %content/scripteditor/
% skin    scripteditor    classic/1.0 %skin/classic/scripteditor/
% overlay   chrome://bluegriffon/content/xul/bluegriffon.xul    chrome://scripteditor/content/scripteditorOverlay.xul
  content/scripteditor/scripteditorOverlay.xul      (content/scripteditorOverlay.xul)
  content/scripteditor/scripteditor.xul             (content/scripteditor.xul)
  content/scripteditor/scripteditor.js              (content/scripteditor.js)
* content/scripteditor/editor.js                    (content/editor.js)
* content/scripteditor/editor.xul                   (content/editor.xul)
  skin/classic/scripteditor/scripteditor.css        (skin/scripteditor.css)
  skin/classic/scripteditor/editor.css              (skin/editor.css)
  skin/classic/scripteditor/plus.png                (skin/plus.png)
  skin/classic/scripteditor/minus.png               (skin/minus.png)
  skin/classic/scripteditor/config.png              (skin/config.png)
  skin/classic/scripteditor/minus-disabled.png      (skin/minus-disabled.png)
  skin/classic/scripteditor/config-disabled.png     (skin/config-disabled.png)
  skin/classic/scripteditor/web.png                 (skin/web.png)
  skin/classic/scripteditor/embedded.png            (skin/embedded.png)
  skin/classic/scripteditor/scripteditor-icon.png   (skin/scripteditor-icon.png)

bluegriffon-en-US.jar:
% locale    scripteditor    en-US		%locale/en-US/scripteditor/
	locale/en-US/scripteditor/scripteditorOverlay.dtd		(locale/en-US/scripteditorOverlay.dtd)
	locale/en-US/scripteditor/scripteditor.dtd		(locale/en-US/scripteditor.dtd)
	locale/en-US/scripteditor/scripteditor.properties		(locale/en-US/scripteditor.properties)
	locale/en-US/scripteditor/editor.dtd          (locale/en-US/editor.dtd)
	
